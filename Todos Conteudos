<body>
    <form id="formVoto" style="background-color: blue;">
        <label for="idade">Digite sua idade:</label>
        <input type="text" id="idade">
        <button type="submit">Verificar</button>
    </form>
    



  <div style="background-color: red;">
    <h2>Resultado</h2>
    <p id="idades"></p>
  </div>

    <script>
         const form = document.getElementById('formVoto');
         form.addEventListener('submit' , function(event){
            event.preventDefault();
            const idade = document.getElementById('idade').value;
            const resultado = document.getElementById('idades');

            alert(idade);


    if (idade >= 18|| idade <=70) {
        resultado.innerText = "Votação Não Obrigatória.";
    } else if ((idade >= 16 && idade == 17) || idade > 70){
        resultado.innerText = "Votação Obrigatoria.";
    } else{
        resultado.innerText = "Dipensado da Votação.";

    }

 

 }
)



    


     </script>

  
